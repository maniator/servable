{
  "name": "servable",
  "version": "0.3.4",
  "description": "From scratch observable",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "rimraf lib dist && buba src -o lib && yarn rollup && uglifyjs dist/index.js -c \"warnings=false\" --comments -m -o dist/index.min.js --verbose",
    "watch": "yarn rollup -w",
    "rollup": "rollup -c",
    "release": "node release.js",
    "exampleDemo": "yarn demo && yarn docs",
    "demo": "yarn rollup -o example/dist/index.js && cp package.json example/",
    "docs": "yarn documentation build src/** -f html -g -o example/docs",
    "server": "http-server"
  },
  "engines": {
    "yarn": "1.10.1",
    "node": "8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maniator/servable.git"
  },
  "author": "Neal Lubin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maniator/servable/issues"
  },
  "homepage": "https://github.com/maniator/servable#readme",
  "devDependencies": {
    "buba": "^4.0.2",
    "documentation": "8.1.2",
    "http-server": "^0.10.0",
    "inquirer": "^3.0.6",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-filesize": "^1.3.2",
    "rollup-plugin-license": "^0.3.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2",
    "semver": "^5.3.0",
    "shelljs": "^0.7.7",
    "uglify-js": "^3.0.3"
  }
}
